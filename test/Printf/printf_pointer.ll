  store ptr addrspace(1) null, ptr %a.addr, align 4
  store ptr addrspace(1) %a, ptr %a.addr, align 4
  %0 = load ptr addrspace(1), ptr %a.addr, align 4
  %call = call spir_func i32 (ptr addrspace(2), ...) @printf(ptr addrspace(2) @.str, ptr addrspace(1) %0) #2
  ret void
}

